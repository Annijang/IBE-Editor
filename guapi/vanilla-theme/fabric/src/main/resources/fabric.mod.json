{
  "schemaVersion": 1,
  "id": "guapi-vanilla-theme",
  "version": "${version}",
  "name": "GUAPI: Vanilla Theme",
  "description": "GUAPI extension to register the 'Vanilla' theme",
  "authors": [
    "Franckyi"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/ibe-editor",
    "sources": "https://github.com/Franckyi/IBE-Editor"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.github.franckyi.guapi.fabric.theme.FabricGuapiVanillaThemeMod"
    ]
  },
  "mixins": [
    "guapi-vanilla-theme-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "gameadapter": "${version}",
    "guapi": "${version}"
  }
}
