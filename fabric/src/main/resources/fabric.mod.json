{
  "schemaVersion": 1,
  "id": "ibeeditor",
  "version": "${version}",
  "name": "IBE Editor",
  "description": "Simple GUI Mod to edit an item, a block or an entity in your current world",
  "authors": [
    "Franckyi"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/ibe-editor",
    "sources": "https://github.com/Franckyi/IBE-Editor",
    "issues": "https://github.com/Franckyi/IBE-Editor/issues"
  },
  "license": "MIT",
  "icon": "logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.franckyi.ibeeditor.fabric.FabricIBEEditorModCommon"
    ],
    "client": [
      "com.github.franckyi.ibeeditor.fabric.FabricIBEEditorModClient"
    ],
    "modmenu": [
      "com.github.franckyi.ibeeditor.fabric.FabricIBEEditorModMenuApiImpl"
    ]
  },
  "mixins": [
    "ibeeditor-common.mixins.json",
    "ibeeditor-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": ">=0.30.0",
    "minecraft": "1.17.x"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/upWJQGd"
      }
    }
  }
}
